<script>
    $('#videoModal').on('show.bs.modal', function (event) {
        var button = $(event.relatedTarget);
        var videoSrc = button.data('src');
        var modal = $(this);
        modal.find('#video').attr('src', videoSrc);
    });

    $('#videoModal').on('hide.bs.modal', function () {
        var modal = $(this);
        modal.find('#video').attr('src', '');
    });
</script>

